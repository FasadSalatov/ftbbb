{
  "name": "football2d",
  "version": "0.0.1",
  "description": "2D Football Engine using HTML Canvas.",
  "keywords": [
    "2d",
    "football",
    "engine",
    "soccer",
    "game",
    "simulator"
  ],
  "author": "Damian Cyntler",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && webpack serve -c ./webpack.config.preview.mjs --open",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && webpack && tsc --emitDeclarationOnly",  
    "lint": "eslint",
    "test": "jest --passWithNoTests",
    "check-prettier": "prettier --check ./src",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^6.0.0",
    "jest": "^27.3.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "webpack-node-externals": "^3.0.0"
  }
}
